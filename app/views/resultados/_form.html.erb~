<%= simple_form_for(@resultado) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :paciente %>
    <%= f.input :pesoinicial %>
    <%= f.input :pesofinal %>
    <%= f.input :status %>
    <%= f.input :perdas %>
    <%= f.input :ganhos %>
  </div>

  <div class="form-actions">
    <%= f.submit 'Salvar' %>
  </div>
<% end %>
