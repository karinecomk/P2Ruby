<p id="notice"><%= notice %></p>

<h1>Listing Objetivos</h1>

<table>
  <thead>
    <tr>
      <th>Dias</th>
      <th>Peso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @objetivos.each do |objetivo| %>
      <tr>
        <td><%= objetivo.dias %></td>
        <td><%= objetivo.peso %></td>
        <td><%= link_to 'Show', objetivo %></td>
        <td><%= link_to 'Edit', edit_objetivo_path(objetivo) %></td>
        <td><%= link_to 'Excluir', objetivo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Objetivo', new_objetivo_path %>
