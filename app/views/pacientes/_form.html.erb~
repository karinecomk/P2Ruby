  <div class="form-inputs">
    <%= f.input :nome %>
    <%= f.input :cpf %>
    <%= f.input :endereco %>
    <%= f.input :nascimento %>
    <%= f.input :altura %>
    <%= f.input :imc %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
